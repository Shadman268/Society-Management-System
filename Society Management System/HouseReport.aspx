<%@ Page Language="C#" AutoEventWireup="true" CodeFile="HouseReport.aspx.cs" Inherits="HouseReport" %>

<!DOCTYPE html>

<html xmlns="http://www.w3.org/1999/xhtml">
<head runat="server">
    <title></title>
</head>
<body>
    <form id="form1" runat="server">
        <div>
